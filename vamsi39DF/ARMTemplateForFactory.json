						{
							"linkedService": {
								"referenceName": "AzureSqlDatabase_1",
								"type": "LinkedServiceReference"
							},
							"name": "saljobpivoting"
						}
					],
					"transformations": [
						{
							"name": "pivot1"
						},
						{
							"name": "select1"
						}
					],
					"scriptLines": [
						"source(output(",
						"          EMPNO as decimal(4,0),",
						"          ENAME as string,",
						"          JOB as string,",
						"          MGR as decimal(4,0),",
						"          HIREDATE as timestamp,",
						"          SAL as decimal(7,2),",
						"          COMM as decimal(7,2),",
						"          DEPTNO as decimal(2,0),",
						"          logDate as date",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     isolationLevel: 'READ_UNCOMMITTED',",
						"     format: 'table') ~> emp",
						"select1 pivot(groupBy(DEPTNO),",
						"     pivotBy(JOB),",
						"     {} = max(SAL),",
						"     columnNaming: '$N$V',",
						"     lateral: false) ~> pivot1",
						"emp select(mapColumn(",
						"          JOB,",
						"          SAL,",
						"          DEPTNO",
						"     ),",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> select1",
						"pivot1 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     format: 'table',",
						"     store: 'sqlserver',",
						"     schemaName: 'dbo',",
						"     tableName: 'salJobPivot',",
						"     insertable: true,",
						"     updateable: false,",
						"     deletable: false,",
						"     upsertable: false,",
						"     recreate: true,",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     errorHandlingOption: 'stopOnFirstError') ~> saljobpivoting"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/AzureSql_1')]",
				"[concat(variables('factoryId'), '/linkedServices/AzureSqlDatabase_1')]"
			]
		},
